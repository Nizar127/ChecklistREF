<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.CEFS.checklist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Checklist">
        <activity android:name=".CompleteActivity"></activity>
        <activity android:name=".ApprovalEquipmentChecklist" />
        <activity android:name=".EquipmentDetailChecklistActivity" />
        <activity android:name=".NextFormUpload" />
        <activity android:name=".AdminDashboardActivity" />
        <activity android:name=".AdminApprovalActivity" />
        <activity android:name=".AuthenticationActivity" />
        <activity android:name=".InsertDataActivity" />
        <activity android:name=".Authentication.AdminLoginActivity" />
        <activity android:name=".Authentication.SignUpActivity" />
        <activity android:name=".Authentication.LoginActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>